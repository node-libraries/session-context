{
  "name": "session-context",
  "version": "0.0.3",
  "main": "./dist/cjs/index.js",
  "types": "./dist/cjs/index.d.ts",
  "exports": {
    "./vite": {
      "default": {
        "types": "./dist/cjs/vitePlugin.d.ts",
        "require": "./dist/cjs/vitePlugin.js",
        "import": "./dist/esm/vitePlugin.js"
      }
    },
    ".": {
      "default": {
        "types": "./dist/cjs/index.d.ts",
        "require": "./dist/cjs/index.js",
        "import": "./dist/esm/index.js"
      }
    }
  },
  "typesVersions": {
    "*": {
      "vite": [
        "./dist/cjs/vitePlugin.d.ts"
      ],
      "*": [
        "./dist/cjs/vitePlugin.d.ts"
      ]
    }
  },
  "scripts": {
    "start": "ts-node test",
    "build": "tsc && tsc --project ./tsconfig.esm.json && cpy esm dist",
    "lint:fix": "eslint --fix src/ && prettier -w src"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/node": "^22.7.9",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.11.0",
    "cpy-cli": "^5.0.0",
    "eslint": "9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.31.0",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.11.0",
    "vite": "^5.4.10"
  },
  "author": "SoraKumo",
  "license": "MIT",
  "repository": "https://github.com/node-libraries/session-context"
}
